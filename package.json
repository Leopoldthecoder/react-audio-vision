{
  "name": "react-audio-visualizer",
  "version": "0.0.1",
  "description": "A react component that visualizes audio files",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint example/*.js src/*.js",
    "dev": "parcel example/index.html",
    "build": "npm run lint && rm -rf lib && parcel build ./src/react-audio-visualizer.js --out-file index.js -d lib --no-minify --no-source-maps"
