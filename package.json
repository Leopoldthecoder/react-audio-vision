  "license": "MIT",
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.2.0",
    "eslint-config-doran": "^1.0.7",
    "eslint-plugin-react": "^7.10.0",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-eslint": "^1.0.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
